{
  "expo": {
    "name": "Tunisys",
    "slug": "Tunisys",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/tun.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/auth.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access camera."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "/assets/tun.png",
          "color": "#ffffff",
          "sounds": [
            "/assets/notification.wav"
          ]
        }
      ]
    ]
,    
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier":"com.Tunisys",
      "config":{
        "googleMapsApiKey":"AIzaSyBHAxszhS42IHk4V7O3Byue2wkCzB4uyQo"
        }
      },
    "android": {
      "useNextNotificationsApi": true,

      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package":"com.Tunisys",
      "config":{
        "googleMaps":{
          "apiKey": "AIzaSyBHAxszhS42IHk4V7O3Byue2wkCzB4uyQo"
        },
        "permissions":["ACCESS_COARSE_LOCATION","ACCESS_FINE_LOCATION","FOREGROUND_SERVICE" ]
      }
    },
    
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
